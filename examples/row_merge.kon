// Demonstrates row spread and origin-aware member lookup
(module
  (row-type T1
    (method a (return int))
    (method b (return str)))
  (row-type T2
    (method b (return int))
    (method c (return bool)))
  (row-type T3
    ..T1
    ..T2
    ..never)
  (class Demo
    (open)
    (method call_b_from_T1
      (return str)
      (body (const str one)))
    (method call_b_from_T2
      (return int)
      (body (const int 2))))
  (run Demo call_b_from_T1)
  (run Demo call_b_from_T2))
